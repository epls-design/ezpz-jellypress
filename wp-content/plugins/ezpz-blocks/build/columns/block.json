{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ezpz/columns",
  "version": "0.1.0",
  "title": "Columns",
  "category": "ezpz-layout",
  "description": "Allows you to add multiple columns to a section.",
  "supports": {
    "html": false,
    "customClassName": true,
    "anchor": true,
    "color": {
      "background": true,
      "text": false
    }
  },
  "attributes": {
    "verticalAlign": {
      "type": "string",
      "default": "top"
    },
    "outerContainer": {
      "type": "string",
      "default": "columns"
    },
    "backgroundColor": {
      "type": "string"
    }
  },
  "providesContext": {
    "ezpz/outerContainer": "outerContainer",
    "ezpz/backgroundColor": "backgroundColor"
  },
  "textdomain": "ezpz-columns",
  "editorScript": "file:./index.js"
}
{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ezpz/text-media",
  "version": "0.2.1",
  "title": "Text & Media",
  "category": "ezpz-layout",
  "description": "Allows you to add a two column layout with text and media (video / image)",
  "supports": {
    "html": false,
    "customClassName": true,
    "anchor": true,
    "color": {
      "background": true,
      "text": false
    }
  },
  "attributes": {
    "allowResize": {
      "type": "boolean",
      "default": false
    },
    "verticalAlign": {
      "type": "string",
      "default": "top"
    },
    "outerContainer": {
      "type": "string",
      "default": "text-media"
    },
    "backgroundColor": {
      "type": "string"
    }
  },
  "providesContext": {
    "allowResize": "allowResize",
    "ezpz/outerContainer": "outerContainer",
    "ezpz/backgroundColor": "backgroundColor"
  },
  "textdomain": "ezpz-textmedia",
  "editorScript": "file:./index.js"
}
	"supports": {
		"html": false,
		"customClassName": false,
		"anchor": false,
		"color": {
			"background": false,
			"text": false
		}
	},
	"attributes": {},
	"textdomain": "ezpz-post-title",
	"editorScript": "file:./index.js"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ezpz/content",
	"version": "0.1.0",
	"title": "Content",
	"category": "text",
	"description": "Use this block to add a mix of different content blocks to your page, for example text, images, etc.",
	"parent": ["ezpz/section", "ezpz/column"],
	"supports": {
		"html": false,
		"customClassName": false
	},
	"attributes": {
		"allowResize": {
			"type": "boolean",
			"default": false
		}
	},
	"providesContext": {
		"allowResize": "allowResize"
	},
	"allowedInnerBlocks": [
		"core/heading",
		"core/paragraph",
		"core/table",
		"core/list",
		"core/image",
		"core/quote",
		"core/audio",
		"core/pullquote",
		"core/embed",
		"core/separator",
		"core/html",
		"core/shortcode",
		"core/code",
		"gravityforms/form",
		"core/separator"
	],
	"textdomain": "ezpz-content",
	"editorScript": "file:./index.js"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ezpz/section",
	"version": "0.1.0",
	"title": "Section",
	"category": "ezpz-layout",
	"description": "Full width section to include text, lists and buttons.",
	"keywords": ["section", "text", "list"],
	"supports": {
		"html": false,
		"customClassName": true,
		"anchor": true,
		"color": {
			"background": true,
			"text": false
		}
	},
	"attributes": {
		"outerContainer": {
			"type": "string",
			"default": "section"
		},
		"backgroundColor": {
			"type": "string",
			"default": "transparent"
		}
	},
	"providesContext": {
		"ezpz/outerContainer": "outerContainer",
		"ezpz/backgroundColor": "backgroundColor"
	},
	"textdomain": "ezpz-section",
	"editorScript": "file:./index.js"
}

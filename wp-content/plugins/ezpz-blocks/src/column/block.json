{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ezpz/column",
	"version": "0.1.0",
	"title": "Column",
	"category": "layout",
	"description": "A single column containing content.",
	"parent": ["ezpz/columns", "ezpz/text-media"],
	"supports": {
		"html": false,
		"customClassName": false,
		"anchor": false,
		"color": {
			"background": false,
			"text": false
		}
	},
	"attributes": {
		"width": {
			"type": "string",
			"default": "50%"
		},
		"defaultContent": {
			"type": "array",
			"default": []
		},
		"restrictContent": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "ezpz-column",
	"editorScript": "file:./index.js"
}

{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ezpz/html",
  "title": "Unfiltered HTML",
  "category": "embed",
  "icon": "html",
  "description": "Renders a block containing unfiltered HTML, useful for adding embeds or other custom HTML such as scripts or widgets.",
  "keywords": ["html", "widget", "embed", "script", "code"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": false,
    "alignText": true,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false
  },
  "styles": [
    { "name": "default", "label": "Default", "isDefault": true },
    { "name": "hide-below-sm", "label": "Hide Mob" },
    { "name": "hide-above-sm", "label": "Hide Above Mob" }
  ],
  "acf": {
    "mode": "auto",
    "renderTemplate": "view.php",
    "postTypes": ["post", "page"]
  }
}

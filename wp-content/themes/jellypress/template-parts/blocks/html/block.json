{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ezpz/html",
  "title": "Unfiltered HTML",
  "category": "embed",
  "icon": "html",
  "description": "Renders a block containing unfiltered HTML, useful for adding embeds or other custom HTML such as scripts or widgets.",
  "keywords": ["html", "widget", "embed", "script", "code"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "parent": ["ezpz/content"],
  "supports": {
    "align": false,
    "alignContent": false,
    "alignText": false,
    "anchor": false,
    "color": {
      "background": false,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": false,
    "html": false
  },
  "styles": [],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": ["post", "page"]
  }
}

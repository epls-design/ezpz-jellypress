{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "jellypress/post-archive",
  "title": "Post Archive",
  "category": "widgets",
  "icon": "archive",
  "description": "Renders a block containing the archive for a specified post type and category.",
  "keywords": ["archive", "post", "category", "feed"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": false,
    "alignText": true,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false,
    "multiple": false
  },
  "styles": [
    { "name": "default", "label": "Default", "isDefault": true },
    { "name": "hide-below-sm", "label": "Hide Mob" },
    { "name": "hide-above-sm", "label": "Hide Above Mob" }
  ],
  "acf": {
    "mode": "auto",
    "renderTemplate": "view.php",
    "postTypes": ["post", "page"]
  }
}

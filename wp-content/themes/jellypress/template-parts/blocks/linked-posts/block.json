{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ezpz/linked-posts",
  "title": "Linked Posts",
  "category": "widgets",
  "icon": "media-archive",
  "description": "Renders a block containing cards linking to other content on the site.",
  "keywords": [
    "cards",
    "card",
    "posts",
    "related",
    "linked",
    "link",
    "widget",
    "content",
    "news",
    "archive"
  ],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": false,
    "alignText": true,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false,
    "multiple": false
  },
  "styles": [],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": ["post", "page"]
  }
}

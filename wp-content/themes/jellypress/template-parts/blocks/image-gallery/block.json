{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "jellypress/image-gallery",
  "title": "Image Gallery",
  "category": "media",
  "icon": "format-gallery",
  "description": "Renders a block containing multiple images in a gallery.",
  "keywords": [
    "gallery",
    "album",
    "photography",
    "photos",
    "images",
    "pictures"
  ],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": ["wide", "full"],
    "alignText": true,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "center"
    }
  },
  "styles": [
    { "name": "default", "label": "Default", "isDefault": true },
    { "name": "hide-below-sm", "label": "Hide Mob" },
    { "name": "hide-above-sm", "label": "Hide Above Mob" }
  ],
  "acf": {
    "mode": "auto",
    "renderTemplate": "view.php",
    "postTypes": ["post", "page"]
  }
}

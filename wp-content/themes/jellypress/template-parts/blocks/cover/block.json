  "keywords": [
    "cover",
    "image",
    "text",
    "overlay",
    "background",
    "parallax",
    "scroll"
  ],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": false,
    "alignContent": "matrix",
    "alignText": true,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "align": {
      "type": "string",

    },
    "outerContainer": {
      "type": "string",
      "default": "cover"
    },
    "backgroundColor": {
      "type": "string",
      "default": "black"
    }
  },
  "providesContext": {
    "ezpz/outerContainer": "outerContainer",
    "ezpz/backgroundColor": "backgroundColor",
    "ezpz/alignText": "alignText"
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "previewImage": "preview.png"
      }
    }
  },
  "styles": [],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": []
  }

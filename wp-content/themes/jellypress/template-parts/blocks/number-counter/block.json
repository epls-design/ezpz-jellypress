{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ezpz/number-counter",
  "title": "Number Counter",
  "category": "widgets",
  "icon": "plus-alt",
  "description": "Renders a block containing a series of numbers that count up to a target value.",
  "keywords": [
    "count",
    "counter",
    "number",
    "animate",
    "animation",
    "counter",
    "impact"
  ],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": false,
    "alignText": true,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "outerContainer": {
      "type": "string",
      "default": "number-counter"
    },
    "backgroundColor": {
      "type": "string",
      "default": "transparent"
    }
  },
  "providesContext": {
    "ezpz/outerContainer": "outerContainer",
    "ezpz/backgroundColor": "backgroundColor",
    "ezpz/alignText": "alignText"
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "previewImage": "preview.png"
      }
    }
  },
  "styles": [],
  "viewScript": ["number-counter"],
  "editorScript": ["number-counter"],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": []
  }
}

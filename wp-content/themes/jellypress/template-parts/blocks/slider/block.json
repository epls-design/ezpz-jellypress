{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ezpz/slider",
  "title": "Slider",
  "category": "widgets",
  "icon": "slides",
  "description": "Renders a block containing a simple slider / carousel.",
  "keywords": ["slider", "carousel", "slide", "image", "slides"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": false,
    "alignContent": true,
    "anchor": true,
    "alignText": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "outerContainer": {
      "type": "string",
      "default": "slider"
    },
    "backgroundColor": {
      "type": "string",
      "default": "transparent"
    }
  },
  "providesContext": {
    "ezpz/outerContainer": "outerContainer",
    "ezpz/backgroundColor": "backgroundColor",
    "ezpz/alignText": "alignText"
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "previewImage": "preview.png"
      }
    }
  },
  "styles": [],
  "viewScript": ["swiper-init"],
  "editorScript": ["swiper-init"],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": []
  }
}

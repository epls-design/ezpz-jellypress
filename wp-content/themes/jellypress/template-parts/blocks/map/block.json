{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ezpz/map",
  "title": "Map",
  "category": "embed",
  "icon": "location-alt",
  "description": "Renders a block containing a map with markers.",
  "keywords": ["map", "google", "marker", "location", "embed"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": ["center", "wide", "full"],
    "alignText": true,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false,
    "multiple": false
  },
  "attributes": {
    "outerContainer": {
      "type": "string",
      "default": "map"
    },
    "backgroundColor": {
      "type": "string",
      "default": "transparent"
    }
  },
  "providesContext": {
    "ezpz/outerContainer": "outerContainer",
    "ezpz/backgroundColor": "backgroundColor",
    "ezpz/alignText": "alignText"
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "previewImage": "preview.png"
      }
    }
  },
  "styles": [],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": []
  }
}

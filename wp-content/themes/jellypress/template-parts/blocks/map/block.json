{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "jellypress/map",
  "title": "Map",
  "category": "embed",
  "icon": "location-alt",
  "description": "Renders a block containing a map with markers.",
  "keywords": ["map", "google", "marker", "location", "embed"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": ["center", "wide", "full"],
    "alignText": true,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false,
    "multiple": false
  },
  "styles": [
    { "name": "default", "label": "Default", "isDefault": true },
    { "name": "hide-below-sm", "label": "Hide Mob" },
    { "name": "hide-above-sm", "label": "Hide Above Mob" }
  ],
  "acf": {
    "mode": "auto",
    "renderTemplate": "view.php",
    "postTypes": ["post", "page"]
  }
}

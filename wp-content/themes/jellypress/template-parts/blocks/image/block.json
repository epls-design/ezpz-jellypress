{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ezpz/image",
  "title": "Image",
  "category": "media",
  "icon": "<svg viewBox=\"0 0 24 24\" xmlns=\"http://www.w3.org/2000/svg\" aria-hidden=\"true\" focusable=\"false\"><path d=\"M19 3H5c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2zM5 4.5h14c.3 0 .5.2.5.5v8.4l-3-2.9c-.3-.3-.8-.3-1 0L11.9 14 9 12c-.3-.2-.6-.2-.8 0l-3.6 2.6V5c-.1-.3.1-.5.4-.5zm14 15H5c-.3 0-.5-.2-.5-.5v-2.4l4.1-3 3 1.9c.3.2.7.2.9-.1L16 12l3.5 3.4V19c0 .3-.2.5-.5.5z\"></path></svg>",
  "description": "Renders a block containing an image for impactful display.",
  "keywords": ["image", "photo", "media", "picture"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": ["center", "wide", "full"],
    "alignText": false,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "center"
    },
    "allowResize": {
      "type": "boolean",
      "default": false
    },
    "outerContainer": {
      "type": "string",
      "default": "image"
    },
    "backgroundColor": {
      "type": "string",
      "default": "transparent"
    }
  },
  "providesContext": {
    "allowResize": "allowResize",
    "ezpz/outerContainer": "outerContainer",
    "ezpz/backgroundColor": "backgroundColor",
    "ezpz/alignText": "alignText"
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "previewImage": "preview.png"
      }
    }
  },
  "styles": [],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": []
  }
}

{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ezpz/hero-page",
  "title": "Page Hero",
  "category": "theme",
  "icon": "<svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" aria-hidden=\"true\" focusable=\"false\"><path d=\"M6 5V18.5911L12 13.8473L18 18.5911V5H6Z\"></path></svg>",
  "description": "The main hero section for a page. Cannot be used more than once per page.",
  "keywords": ["hero", "heading", "title", "header"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": false,
    "alignContent": false,
    "alignText": false,
    "anchor": false,
    "color": {
      "background": false,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": false,
    "html": false,
    "multiple": false
  },
  "attributes": {
    "outerContainer": {
      "type": "string",
      "default": "hero-page"
    },
    "backgroundColor": {
      "type": "string",
      "default": "transparent"
    }
  },
  "providesContext": {
    "ezpz/outerContainer": "outerContainer",
    "ezpz/backgroundColor": "backgroundColor",
    "ezpz/alignText": "alignText"
  },
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": ["page"]
  }
}

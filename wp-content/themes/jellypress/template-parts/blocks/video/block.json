{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ezpz/video",
  "title": "Video",
  "category": "embed",
  "icon": "format-video",
  "description": "Renders a block containing a Youtube or Vimeo video.",
  "keywords": ["video", "youtube", "vimeo", "embed", "media", "player"],
  "version": "1.0.0",
  "textdomain": "jellypress",
  "supports": {
    "align": ["center", "wide", "full"],
    "alignText": false,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "link": false,
      "text": false
    },
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "center"
    }
  },
  "styles": [],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": ["post", "page"]
  }
}
